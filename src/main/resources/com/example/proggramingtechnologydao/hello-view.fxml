<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" prefHeight="247.0" prefWidth="442.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proggramingtechnologydao.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <ListView fx:id="listAccount" prefHeight="207.0" prefWidth="290.0" />
      <VBox prefHeight="207.0" prefWidth="121.0">
         <children>
            <TextField fx:id="idAccount" prefHeight="25.0" prefWidth="133.0" />
            <Button fx:id="searchButton" mnemonicParsing="false" onAction="#searchButtonHandler" prefHeight="25.0" prefWidth="122.0" text="Поиск" />
            <Separator prefHeight="16.0" prefWidth="121.0" />
            <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteButtonHandler" prefHeight="25.0" prefWidth="123.0" text="Удалить" />
         </children>
      </VBox>
   </children>
</HBox>
